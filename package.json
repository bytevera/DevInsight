{
  "name": "devinsight",
  "version": "0.1.0",
  "description": "Intelligent runtime debugging & code insight engine for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsc && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir dist/esm && node scripts/fix-esm.js",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "debugging",
    "runtime",
    "error-tracking",
    "async-tracing",
    "developer-tools",
    "observability",
    "diagnostics",
    "nodejs",
    "typescript"
  ],
  "author": "DevInsight Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devinsight/devinsight.git"
  },
  "bugs": {
    "url": "https://github.com/devinsight/devinsight/issues"
  },
  "homepage": "https://github.com/devinsight/devinsight#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "devinsight": "file:devinsight-0.1.0.tgz"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
